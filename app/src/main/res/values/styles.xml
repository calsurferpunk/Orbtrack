    <!--Divider-->
    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">?attr/dividerLineColor</item>
    </style>

    <style name="DividerThin" parent="Divider">
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="DividerCategory" parent="Preference.Category">
        <item name="layout">@layout/divider_view</item>
    </style>

    <!-- Material button -->
    <style name="MaterialButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">bottom|center</item>
        <item name="android:layout_margin">6dp</item>
        <item name="backgroundTint">?attr/pageHighlightBackground</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <!-- Detail Views -->
    <style name="DetailTableMaterial">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="DetailTableRow">
        <item name="android:padding">1dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">?attr/pageBackground</item>
    </style>
    <style name="DetailTableRowMaterial" parent="DetailTableRow">
        <item name="android:background">@color/transparent</item>
    </style>

    <style name="DetailText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">top</item>
        <item name="android:padding">1dp</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <style name="DetailTextMaterial" parent="DetailText">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="DetailTitle" parent="DetailText">
        <item name="android:alpha">0.65</item>
        <item name="android:layout_gravity">top</item>
        <item name="android:gravity">right</item>
    </style>

    <style name="DetailButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_margin">8dp</item>
    </style>

    <style name="DetailVerticalDivider">
        <item name="android:layout_width">2dp</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginRight">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?attr/dividerLineColor</item>
    </style>

    <!--Edit Dialog -->
    <style name="EditValueTitle">
        <item name="android:layout_gravity">right|center_vertical</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Small</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <!--Calculate View-->
    <style name="CalculateEditRow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="CalculateEditLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="CalculateEditText">
        <item name="android:layout_width">75dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:inputType">numberDecimal</item>
        <item name="android:maxLines">1</item>
        <item name="android:maxLength">6</item>
        <item name="android:hint">@string/empty</item>
        <item name="android:selectAllOnFocus">true</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Medium</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <style name="CalculateEditUnitTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Small</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <style name="CalculateEditValueTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Small</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <!--Combined Layout-->
    <style name="CombinedLayoutMaterial">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingRight">15dp</item>
    </style>
    <style name="CombinedLayoutStartMaterial" parent="CombinedLayoutMaterial">
        <item name="android:minWidth">154dp</item>
    </style>

    <!--Combined Image-->
    <style name="CombinedImage">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_margin">1dp</item>
        <item name="android:layout_gravity">center_vertical|end</item>
        <item name="android:gravity">center</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="tint">?attr/defaultTextColor</item>
    </style>
    <style name="CombinedImageMaterial" parent="CombinedImage">
        <item name="android:layout_width">18dp</item>
        <item name="android:layout_height">18dp</item>
    </style>

    <!--Combined Title Text-->
    <style name="CombinedTitleText">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_margin">1dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <!--Combined Text-->
    <style name="CombinedText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">0.5</item>
        <item name="android:padding">2dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <style name="CombinedTextMaterial">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">4dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <!--Title Text-->
    <style name="TitleText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Medium</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">?attr/columnBackground</item>
        <item name="android:textColor">?attr/columnTitleTextColor</item>
    </style>

    <!--Progress circle-->
    <style name="ProgressCircle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:indeterminate">true</item>
    </style>

    <style name="ProgressCircleSmall" parent="ProgressCircle">
        <item name="indicatorSize">16dp</item>
        <item name="trackThickness">2dp</item>
    </style>

    <style name="ProgressLineSmall">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:padding">0dp</item>
        <item name="indicatorColor">?attr/colorAccent</item>
        <item name="trackThickness" tools:ignore="PxUsage">3px</item>
    </style>

    <!--Progress Box-->
    <style name="ProgressBox">
        <item name="android:layout_width">15dp</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:background">@color/light_gray</item>
    </style>

    <style name="ProgressBoxLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!--Floating button-->
    <style name="FloatingButton" parent="Widget.MaterialComponents.FloatingActionButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="useCompatPadding">true</item>
        <item name="borderWidth">0dp</item>
        <item name="fabSize">normal</item>
        <item name="tint">?attr/pageTitleTextColor</item>
        <item name="backgroundTint">?attr/colorAccent</item>
        <item name="android:contentDescription">@string/title_unknown</item>
    </style>

    <!--Result button-->
    <style name="ResultButton" parent="MaterialButton">
        <item name="android:layout_width">130dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ResultButtonLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">-26dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_horizontal|bottom</item>
        <item name="android:padding">6dp</item>
    </style>

    <!--Color button-->
    <style name="ColorButton">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:focusable">false</item>
    </style>

    <!--Visible button-->
    <style name="VisibleButton">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:focusable">false</item>
    </style>

    <!--Radio button-->
    <style name="RadioButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Small</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <!--Notify settings-->
    <style name="NotifySettingsLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="NotifySettingsText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Medium</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <style name="NotifySettingsSwitch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Small</item>
    </style>

    <style name="NotifySettingsRadioGroup">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">end</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">5dp</item>
        <item name="android:visibility">gone</item>
    </style>

    <!--Notify item-->
    <style name="NotifyItemTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="NotifyItemText" parent="NotifyItemTitle">
        <item name="android:paddingRight">3dp</item>
    </style>

    <!--Widgets-->
    <style name="WidgetImage">
        <item name="android:padding">2dp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:maxWidth">96dp</item>
        <item name="android:maxHeight">96dp</item>
    </style>

    <style name="WidgetBarText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">0dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="WidgetDetailText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">0.333</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">0dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="WidgetSetupGlobalSwitch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">16dp</item>
        <item name="android:text">@string/title_shared</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Small</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
        <item name="android:checked">true</item>
    </style>

    <style name="WidgetSetupText">
        <item name="android:minWidth">56sp</item>
        <item name="android:maxWidth">76sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:gravity">center_vertical|end</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Small</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <style name="WidgetSetupTextOptions">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="WidgetSetupCheckBox">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <style name="WidgetSetupBoldCheckBox" parent="WidgetSetupCheckBox">
        <item name="android:text">@string/title_bold</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="WidgetSetupItalicCheckBox" parent="WidgetSetupCheckBox">
        <item name="android:layout_marginLeft">3dp</item>
        <item name="android:text">@string/title_italic</item>
        <item name="android:textStyle">italic</item>
    </style>
</resources>
