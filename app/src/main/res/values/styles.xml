    <!-- Splash screens -->
    <style name="SplashScreen" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimaryDark">@color/transparent</item>
        <item name="android:windowBackground">@drawable/splash_screen_app</item>
    </style>
    <style name="GoogleDriveSplashScreen" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimaryDark">@color/transparent</item>
        <item name="android:windowBackground">@drawable/splash_screen_google_drive</item>
    </style>
    <style name="DropboxSplashScreen" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimaryDark">@color/transparent</item>
        <item name="android:windowBackground">@drawable/splash_screen_dropbox</item>
    </style>

    <!-- Dialogs -->
    <style name="DialogButton" parent="Widget.MaterialComponents.Button.TextButton.Dialog">
        <item name="android:textColor">?attr/defaultTextColor</item>
    </style>

    <style name="DialogBase" parent="ThemeOverlay.MaterialComponents.Dialog.Alert">
        <item name="android:background">@color/transparent</item>
        <item name="android:selectableItemBackground">?attr/colorAccent</item>
        <item name="buttonBarPositiveButtonStyle">@style/DialogButton</item>
        <item name="buttonBarNeutralButtonStyle">@style/DialogButton</item>
        <item name="buttonBarNegativeButtonStyle">@style/DialogButton</item>
        <item name="colorControlActivated">?attr/colorAccent</item>
        <item name="materialButtonStyle">@style/DialogButton</item>
    </style>
    
    <style name="LightDialog" parent="DialogBase">
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorPrimary">@color/black</item>
        <item name="android:textColorSecondary">@color/dark_gray</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="colorControlNormal">@color/black</item>
    </style>

    <style name="DarkDialog" parent="DialogBase">
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorPrimary">@color/grey</item>
        <item name="android:textColorSecondary">@color/very_light_gray</item>
        <item name="android:windowBackground">@color/very_dark_gray</item>
        <item name="colorControlNormal">@color/white</item>
    </style>

    <!-- Preferences -->
    <style name="LightPreferenceOverlay" parent="PreferenceThemeOverlay">
        <!--item name="android:background">@color/white</item-->
        <item name="android:textColor">@color/black</item>
        <item name="android:tint">@color/black</item>
