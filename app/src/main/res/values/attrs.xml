<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="defaultTextColor" format="reference"/>
    <attr name="defaultTextShadowColor" format="reference"/>
    <attr name="actionBarBackground" format="reference"/>
    <attr name="pageTitleTextColor" format="reference"/>
    <attr name="pageTitleBackground" format="reference"/>
    <attr name="pageBackground" format="reference"/>
    <attr name="pageHighlightBackground" format="reference"/>
    <attr name="viewPagerBackground" format="reference"/>
    <attr name="columnTitleTextColor" format="reference"/>
    <attr name="columnTitleTextSelectColor" format="reference"/>
    <attr name="columnBackground" format="reference"/>
    <attr name="dividerColor" format="reference"/>
    <attr name="dividerTextBackground" format="color"/>
    <attr name="dialogStyle" format="reference"/>
    <attr name="itemSelect" format="reference"/>
    <attr name="itemSelected" format="reference"/>
    <attr name="normalTint" format="color"/>
    <attr name="checkedTint" format="color"/>
    <attr name="sharedName" format="string"/>
    <attr name="titleText" format="string"/>
    <attr name="minValue" format="float"/>
    <attr name="maxValue" format="float"/>

    <declare-styleable name="PlayBar">
        <attr name="textColor" format="color"/>
        <attr name="buttonColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="Graph">
        <attr name="dataTextSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="FloatingActionStateButton">
        <attr name="normalTint"/>
        <attr name="checkedTint"/>
    </declare-styleable>

    <declare-styleable name="FloatingActionStateButtonMenu">
        <attr name="imageTint" format="color"/>
        <attr name="backgroundTint" format="color"/>
        <attr name="menuImageId" format="integer"/>
        <attr name="normalTint"/>
        <attr name="checkedTint"/>
    </declare-styleable>

    <declare-styleable name="TimeIntervalPreference">
        <attr name="sharedName"/>
        <attr name="hourKey" format="string"/>
        <attr name="minuteKey" format="string"/>
        <attr name="intervalKey" format="string"/>
    </declare-styleable>

    <declare-styleable name="IconListPreference">
        <attr name="sharedName"/>
        <attr name="titleText"/>
    </declare-styleable>

    <declare-styleable name="RadioGroupPreference">
        <attr name="sharedName"/>
        <attr name="titleText"/>
        <attr name="itemTexts" format="reference"/>
        <attr name="itemValues" format="reference"/>
        <attr name="valueType" format="enum">
            <enum name="String" value="0"/>
            <enum name="Boolean" value="1"/>
            <enum name="Byte" value="2"/>
            <enum name="Integer" value="3"/>
            <enum name="Long" value="4"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="SwitchButtonPreference">
        <attr name="sharedName"/>
        <attr name="titleText"/>
    </declare-styleable>

    <declare-styleable name="SwitchTextPreference">
        <attr name="sharedName"/>
        <attr name="switchKey" format="string"/>
        <attr name="titleText"/>
        <attr name="suffixText" format="string"/>
        <attr name="showSwitch" format="boolean"/>
        <attr name="minValue"/>
        <attr name="maxValue"/>
    </declare-styleable>

    <declare-styleable name="SliderPreference">
        <attr name="sharedName"/>
        <attr name="titleText"/>
        <attr name="minValue"/>
        <attr name="maxValue"/>
        <attr name="scaleType" format="enum">
            <enum name="Integer" value="0"/>
            <enum name="Percent" value="1"/>
        </attr>
    </declare-styleable>
</resources>